<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<constant name="struts.devMode" value="true" />
<!-- 默认的视图主题 -->
    <constant name="struts.ui.theme" value="simple" />
    <constant name="struts.objectFactory" value="spring" />
	

    <!-- Add packages here -->
    
    <package name="diction" namespace="/dictionManage" extends="struts-default">
		<action name="diction" class="dictionAction" >
			<result name="view">/dictionaryManage/dictionManage.jsp</result>
			<result name="detail">/dictionaryManage/diction_view.jsp</result>
			<result name="add">/dictionaryManage/diction_add.jsp</result>
		</action> 

    </package>

</struts>
