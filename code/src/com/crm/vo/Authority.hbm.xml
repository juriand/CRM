<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.crm.vo">
	<class name="Authority" table="CRM_AUTHORITY">
		<id name="authority_id" column="AUTHORITY_ID" type="string">
		    <generator class="sequence">
				<param name="sequence">CRM_AUTHORITY_SEQ</param> 
			</generator>
		</id>
		
		<property name="authority_name" column="AUTHORITY_NAME" type="string"/>

	</class>
</hibernate-mapping>